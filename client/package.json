{
  "name": "excel_compare_and_import",
  "version": "3.1.4",
  "type": "module",
  "description": "This is a Base extension for Feishu that compares local Excel files with base and imports the differences into the current Base.",
  "scripts": {
    "dev": "vite --mode dev",
    "dev:Beta": "vite --mode beta",
    "github:build": "vite build --base /Excel-Compare-and-Import/ --mode alpha",
    "build": "vite build --base ./ --mode stable",
    "preview": "vite preview --port 4173",
    "createData": "npx vite-node ./scripts/createTestData.ts -n",
    "prepare": "husky",
    "style": "npx prettier . --write",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:u": "vitest -u",
    "typecheck": "vitest typecheck",
    "generate:sdk": "openapi-ts"
  },
  "keywords": [
    "Excel",
    "Base",
    "Bitable",
    "Import",
    "Compare"
  ],
  "output": "dist",
  "author": {
    "name": "qww",
    "url": "https://github.com/497363983",
    "email": "wqian9790@gmail.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/use-bitable/Excel-Compare-and-Import.git"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.1.0",
    "@lark-base-open/js-sdk": "^0.5.0",
    "@qww0302/use-bitable": "^1.0.0-alpha.0",
    "@unhead/vue": "^2.0.0",
    "@vueuse/core": "^13.0.0",
    "dayjs": "^1.11.9",
    "element-plus": "^2.4.0",
    "highlight.js": "^11.9.0",
    "mathjs": "^14.0.0",
    "vue": "^3.3.4",
    "vue-i18n": "^11.0.0",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.20.3/xlsx-0.20.3.tgz"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.0.0",
    "@commitlint/config-conventional": "^19.0.0",
    "@hey-api/openapi-ts": "^0.71.0",
    "@types/fs-extra": "^11.0.1",
    "@types/node": "^22.0.0",
    "@vitejs/plugin-vue": "^5.0.0",
    "@vitest/coverage-v8": "^3.0.0",
    "@vitest/ui": "^3.0.0",
    "commander": "^14.0.0",
    "fs-extra": "^11.1.1",
    "husky": "^9.0.0",
    "jsdom": "^26.0.0",
    "lint-staged": "^16.0.0",
    "mockjs": "^1.1.0",
    "msw": "^2.0.11",
    "npm-run-all2": "^8.0.0",
    "prettier": "^3.0.3",
    "rollup-plugin-visualizer": "^6.0.0",
    "typescript": "^5.5.4",
    "unplugin-auto-import": "^19.0.0",
    "unplugin-vue-components": "^28.0.0",
    "vite": "^6.0.0",
    "vitest": "^3.0.0"
  },
  "lint-staged": {
    "*.{js,css,md,ts,vue,json}": "prettier --write"
  },
  "bugs": {
    "url": "https://github.com/use-bitable/Excel-Compare-and-Import/issues"
  },
  "homepage": "https://github.com/use-bitable/Excel-Compare-and-Import/blob/main/README.md"
}
