{
  "name": "excel_compare_and_import",
  "version": "3.1.1",
  "type": "module",
  "description": "This is a Base extension for Feishu that compares local Excel files with base and imports the differences into the current Base.",
  "scripts": {
    "dev": "vite --mode dev",
    "dev:Beta": "vite --mode beta",
    "github:build": "vite build --base /Excel-Compare-and-Import/ --mode alpha",
    "build": "vite build --base ./ --mode stable",
    "preview": "vite preview --port 4173",
    "createData": "npx vite-node ./scripts/createTestData.ts -n",
    "prepare": "husky install",
    "style": "npx prettier . --write",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:u": "vitest -u",
    "typecheck": "vitest typecheck"
  },
  "keywords": [
    "Excel",
    "Base",
    "Bitable",
    "Import",
    "Compare"
  ],
  "author": {
    "name": "qww",
    "url": "https://github.com/497363983"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/zjut-bio-party-undergraduate-branch/Excel-Compare-and-Import.git"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.1.0",
    "@lark-base-open/js-sdk": "^0.3.5",
    "@qww0302/use-bitable": "^0.2.0",
    "@unhead/vue": "^1.7.3",
    "@vueuse/core": "^10.3.0",
    "dayjs": "^1.11.9",
    "element-plus": "^2.4.0",
    "highlight.js": "^11.9.0",
    "mathjs": "^12.0.0",
    "vue": "^3.3.4",
    "vue-i18n": "^9.6.0",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.20.0/xlsx-0.20.0.tgz"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.0.0",
    "@commitlint/config-conventional": "^19.0.0",
    "@types/fs-extra": "^11.0.1",
    "@types/node": "^20.4.10",
    "@vitejs/plugin-vue": "^5.0.0",
    "@vitest/coverage-v8": "^1.0.0",
    "@vitest/ui": "^1.0.0",
    "commander": "^12.0.0",
    "fs-extra": "^11.1.1",
    "husky": "^9.0.0",
    "jsdom": "^24.0.0",
    "lint-staged": "^15.0.0",
    "mockjs": "^1.1.0",
    "msw": "^2.0.11",
    "npm-run-all2": "^6.0.0",
    "prettier": "^3.0.3",
    "rollup-plugin-visualizer": "^5.9.2",
    "typescript": "^5.1.6",
    "unplugin-auto-import": "^0.17.0",
    "unplugin-vue-components": "^0.26.0",
    "vite": "^5.0.0",
    "vitest": "^1.0.0"
  },
  "lint-staged": {
    "*.{js,css,md,ts,vue,json}": "prettier --write"
  },
  "bugs": {
    "url": "https://github.com/zjut-bio-party-undergraduate-branch/Excel-Compare-and-Import/issues"
  },
  "homepage": "https://github.com/zjut-bio-party-undergraduate-branch/Excel-Compare-and-Import#readme"
}
